{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Unified-System-of-Academic-Management": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "usam",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Unified-System-of-Academic-Management",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/usamicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/bower_components/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/bower_components/font-awesome/css/font-awesome.min.css",
              "src/assets/bower_components/Ionicons/css/ionicons.min.css",
              "src/assets/css/AdminLTE.min.css",
              "src/assets/css/skins/_all-skins.min.css"
            ],
            "scripts": [
              "src/assets/bower_components/jquery/dist/jquery.min.js",
              "src/assets/bower_components/jquery/dist/jquery-migrate-3.0.0.min.js",
              "src/assets/bower_components/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/bower_components/jquery-ui/jquery-ui.min.js",
              "src/assets/bower_components/jquery-slimscroll/jquery.slimscroll.min.js",
              "src/assets/bower_components/fastclick/lib/fastclick.js",
              "src/assets/js/adminlte.min.js",
              "src/assets/js/demo.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Unified-System-of-Academic-Management:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "Unified-System-of-Academic-Management:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Unified-System-of-Academic-Management:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/usamicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "Unified-System-of-Academic-Management-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "Unified-System-of-Academic-Management:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "Unified-System-of-Academic-Management:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "Unified-System-of-Academic-Management"
}